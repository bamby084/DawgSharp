      "frameworkAssemblies": {
        "System": { "type": "build" },
        "System.Core": { "type": "build" }
      }
    },
    "net35": {
      "frameworkAssemblies": {
        "System": { "type": "build" },
        "System.Core": { "type": "build" }
      }
    },
    "netstandard1.6": {
      "imports": "dnxcore50",
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
